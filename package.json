{
  "name": "@daniloster/svelte-i18n",
  "version": "0.0.1",
  "description": "Library to apply internationalization with interpolation of strings and components",
  "main": "lib/main.js",
  "scripts": {
    "build": "NODE_ENV=production webpack --config tools/webpack/webpack.svelte.config.js --mode production",
    "dev": "NODE_ENV=development webpack-dev-server --config tools/webpack/webpack.svelte.config.js --mode development",
    "test": "cross-env NODE_ENV=test jest --config jest.config.js",
    "pretest:coverage": "rm -rf ./.database",
    "test:coverage": "cross-env NODE_ENV=test jest --config jest.config.coverage.js",
    "prerelease": "yarn build"
  },
  "repository": {
    "url": "https://github.com/daniloster/svelte-i18n"
  },
  "keywords": [
    "Svelte",
    "i18n",
    "component",
    "library"
  ],
  "author": "daniloster",
  "devDependencies": {
    "@babel/core": "7.8.7",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-export-namespace-from": "7.8.3",
    "@babel/plugin-proposal-throw-expressions": "7.8.3",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-modules-umd": "7.8.3",
    "@babel/polyfill": "7.8.7",
    "@babel/preset-env": "7.8.7",
    "@babel/runtime": "7.8.7",
    "@testing-library/jest-dom": "5.5.0",
    "@testing-library/svelte": "3.0.0",
    "@testing-library/user-event": "10.0.1",
    "babel-jest": "24.8.0",
    "babel-loader": "8.1.0",
    "cross-env": "7.0.2",
    "file-loader": "6.0.0",
    "full-icu": "1.3.1",
    "html-webpack-plugin": "4.2.0",
    "jest": "24.9.0",
    "prettier": "2.0.5",
    "prettier-plugin-svelte": "0.7.0",
    "source-map-support": "0.5.16",
    "svelte-jester": "1.0.5",
    "svelte-loader": "2.13.6",
    "url-loader": "4.1.0",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3",
    "worker-plugin": "4.0.2"
  },
  "optionalDependencies": {
    "appdmg": "0.6.0"
  },
  "dependencies": {
    "mutation-helper": "1.0.0",
    "svelte": "3.21.0"
  }
}
